{
  "name": "backend-testing-framework-junior",
  "version": "1.0.0",
  "description": "Simple backend testing framework for junior developers using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:api": "playwright test tests/api",
    "test:integration": "playwright test tests/integration",
    "test:examples": "playwright test tests/examples",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "setup": "node scripts/setup.js",
    "generate": "node scripts/generate-test.js",
    "install:browsers": "playwright install"
  },
  "keywords": [
    "testing",
    "api-testing",
    "backend-testing",
    "playwright",
    "junior-developer",
    "learning"
  ],
  "author": "Backend Testing Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "dependencies": {
    "dotenv": "^16.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
